{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": [
    "input"
  ],
  "properties": {
    "input": {
      "type": "object",
      "required": [
        "seed",
        "format"
      ],
      "properties": {
        "seed": {
          "type": "integer",
          "description": "Random seed number"
        },
        "format": {
          "enum": [
            "json",
            "xml",
            "csv"
          ],
          "type": "string",
          "description": "Output format"
        }
      },
      "additionalProperties": false
    },
    "output": {
      "type": "object",
      "properties": {
        "user_age": {
          "type": "integer"
        },
        "user_email": {
          "type": "string"
        },
        "user_active": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}