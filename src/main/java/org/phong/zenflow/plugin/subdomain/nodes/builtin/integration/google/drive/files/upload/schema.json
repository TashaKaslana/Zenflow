{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Google Drive Upload File Schema",
  "properties": {
    "input": {
      "type": "object",
      "title": "Input Schema",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the file to create"
        },
        "mimeType": {
          "type": "string",
          "description": "MIME type of the file",
          "default": "application/octet-stream"
        },
        "data": {
          "type": "string",
          "description": "Base64 encoded file content"
        }
      },
      "required": [
        "name",
        "data"
      ],
      "additionalProperties": false
    },
    "output": {
      "type": "object",
      "title": "Output Schema"
    },
    "profile": {
      "profileKeys": [
        "oauth-default"
      ],
      "description": "Requires the Google OAuth profile for authentication."
    }
  },
  "required": [
    "input",
    "output"
  ],
  "additionalProperties": false
}
